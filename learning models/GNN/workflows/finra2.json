{"Comment": "A Hello World example of the Amazon States Language using Pass states",
 "Name": "Finra",
 "StartAt": "FetchData",
 "States": {"FetchData": {"Type": "Parallel",
   "Branches": [{"States": {"marketdata": {"Resource": "marketdata3",
       "Type": "Task",
       "End": true}},
     "StartAt": "marketdata"},
    {"States": {"diskio": {"Resource": "disk-io",
       "Type": "Task",
       "End": true}},
     "StartAt": "diskio"},
    {"States": {"factorial": {"Resource": "factorial", "Type": "Task", "End": true}},
     "StartAt": "factorial"},
    {"States": {"fibonacci": {"Resource": "fibonacci",
       "Type": "Task",
       "End": true}},
     "StartAt": "fibonacci"},
    {"States": {"pi": {"Resource": "pi",
       "Type": "Task",
       "End": true}},
     "StartAt": "pi"}],
   "Next": "CheckMarginBalance"},
  "CheckMarginBalance": {"Type": "Task",
   "Resource": "margin-balance",
   "End": true}}}