{"Comment": "Media Microservices of DeathStarBench",
 "Name": "MovieReviewing",
 "StartAt": "ComposeReview",
   "States": {"ComposeReview": {"Type": "Task",
   "Resource": "compose-review",
   "Next": "Upload"},
  "Upload": {"Type": "Parallel",
   "Branches": [{"States": {"UploadUserId": {"Resource": "upload-user-id",
       "Type": "Task",
       "End": true}},
     "StartAt": "UploadUserId"},
    {"States": {"UploadMovieid": {"Resource": "upload-movie-id",
       "Type": "Task",
       "End": true}},
     "StartAt": "UploadMovieid"},
    {"States": {"MRUploadText": {"Resource": "mr-upload-text", "Type": "Task", "End": true}},
     "StartAt": "MRUploadText"},
    {"States": {"MRUploadUniqueId": {"Resource": "mr-upload-unique-id",
       "Type": "Task",
       "End": true}},
     "StartAt": "MRUploadUniqueId"}],
   "Next": "ComposeAndUpload"},
  "ComposeAndUpload": {"Type": "Task",
   "Resource": "mr-compose-and-upload",
   "Next": "Store"},
  "Store": {"Type": "Parallel",
   "Branches": [{"States": {"StoreReview": {"Resource": "store-review",
       "Type": "Task",
       "End": true}},
     "StartAt": "StoreReview"},
    {"States": {"UploadUserReview": {"Resource": "upload-user-review",
       "Type": "Task",
       "End": true}},
     "StartAt": "UploadUserReview"},
    {"States": {"UploadMovieReview": {"Resource": "upload-movie-review", "Type": "Task", "End": true}},
     "StartAt": "UploadMovieReview"}],
   "End": true}}}
