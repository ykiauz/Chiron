{"Comment": "Finra Workflow with 5 Parallel Functions",
 "Name": "Finra",
 "StartAt": "FetchData",
 "States": {"FetchData": {"Type": "Parallel",
   "Branches": [{"States": {"marketdata": {"Resource": "marketdata",
       "Type": "Task",
       "End": true}},
     "StartAt": "marketdata"},
    {"States": {"lastpx": {"Resource": "lastpx",
       "Type": "Task",
       "End": true}},
     "StartAt": "lastpx"},
    {"States": {"side": {"Resource": "side", "Type": "Task", "End": true}},
     "StartAt": "side"},
    {"States": {"trddate": {"Resource": "trddate",
       "Type": "Task",
       "End": true}},
     "StartAt": "trddate"},
    {"States": {"volume": {"Resource": "volume",
       "Type": "Task",
       "End": true}},
     "StartAt": "volume"}],
   "Next": "CheckMarginBalance"},
  "CheckMarginBalance": {"Type": "Task",
   "Resource": "margin-balance",
   "End": true}}}
